{
    "printWidth": 200,
    "overrides": [
        {
            "files": ".prettierrc",
            "options": { "parser": "json" }
        }
    ],
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "proseWrap": "preserve",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "eslintIntegration": false,
    "htmlWhitespaceSensitivity": "ignore",
    "ignorePath": ".prettierignore",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "requireConfig": false,
    "stylelintIntegration": false,
    "trailingComma": "es5",
    "prettier.tslintIntegration": false
}
